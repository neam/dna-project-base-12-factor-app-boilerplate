exclude .git

# Use build filters as base
merge .stack.build.filter.rules

# Exclude vendor directories
exclude vendor

# Exclude base-modules
exclude /external-apis/rest-api/*

# Exclude things specifically not shared
exclude /dna/tests/codeception/_data/
exclude /ui/
exclude /tools
exclude /dna/product-specific-dna/
exclude /dna/generated-migrations/PropelMigration_*_foo_*.php
exclude /dna/db/migration*/*/

# Exclude generated base classes
exclude /dna/generated-classes/propel/models/Base
exclude /dna/generated-classes/propel/models/Map
exclude /dna/models/base
exclude /external-apis/rest-api-dna/app/modules/v0/models/base
